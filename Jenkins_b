pipeline{
agent  any
 stages{
      stage ('build'){
         steps{
         sh 'mvn compile'
            echo 'building..'
          }
      }

      stage ('test'){
     steps{
     sh'mvn test'
           echo 'testing..'
       }
      }

     stage ('deploy'){
      steps{
      sh 'mvn deploy'
        echo 'deploying..'
      }
     }
    }
  }
